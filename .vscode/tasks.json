{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "type": "cargo",
      "command": "build",
      "args": [
        "--bin",
        "main",
        "--manifest-path",
        "${workspaceFolder}/rust/Cargo.toml"
      ],
      "problemMatcher": ["$rustc"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
